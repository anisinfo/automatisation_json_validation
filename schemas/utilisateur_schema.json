{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Utilisateur",
    "type": "object",
    "properties": {
      "nom": {
        "type": "string",
        "minLength": 1
      },
      "age": {
        "type": "integer",
        "minimum": 0
      },
      "email": {
        "type": "string",
        "format": "email"
      }
    },
    "required": ["nom", "age"]
  }